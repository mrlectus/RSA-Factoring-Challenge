#!/usr/bin/python3
from sys import argv
from math import sqrt
from multiprocessing import Pool
from factors import find_factor

def main():
    with Pool(8) as pool:
        with open(argv[1], "r") as file:
                lists = pool.map(find_factor, [int(x) for x in file.readlines()])
                for x,y in lists:
                    print(f"{x*y}={x}*{y}")

if __name__ == "__main__":
    main()
